.key_container {
    position: fixed;
    bottom: 0;
    width: 100vw;
    height: 250px;
    line-height: 50px;
    background-color: #fff;
    z-index: 999;
}

.key_02,
.key_03,
.key_04,
.key_05 {
    display: flex;
}

.key_01 {
    display: flex;
    justify-content: space-between;
}

.key_item {
    width: 25%;
    text-align: center;
    border: 1px solid #DFDFDF;
}

.key_05 :nth-child(4) {
    background-color: #69EAFF;
}